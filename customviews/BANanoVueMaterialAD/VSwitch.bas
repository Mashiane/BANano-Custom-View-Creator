'set on UpdateError event, updates the master events records
Sub SetOnUpdateError()
Dim sName As String = $"${mEventName}_UpdateError"$
sName = sName.tolowercase
If SubExists(mCallBack, sName) = False Then Return
If BANano.IsUndefined(eOnUpdateError) Or BANano.IsNull(eOnUpdateError) Then eOnUpdateError = ""
Dim sCode As String = $"${sName}(${eOnUpdateError})"$
AddAttr("v-on:update:error", sCode)
'arguments for the event
Dim argument As Boolean 'ignore
Dim cb As BANanoObject = BANano.CallBack(mCallBack, sName, Array(argument As Boolean))
methods.Put(sName, cb)
End Sub

Sub SetOnUpdateErrorE(sUpdateError As String)
eOnUpdateError = sUpdateError
End Sub

